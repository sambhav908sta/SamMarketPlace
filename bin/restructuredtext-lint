#!/webapps/mysite/bin/python3.8
# EASY-INSTALL-ENTRY-SCRIPT: 'restructuredtext-lint==1.3.2','console_scripts','restructuredtext-lint'
__requires__ = 'restructuredtext-lint==1.3.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('restructuredtext-lint==1.3.2', 'console_scripts', 'restructuredtext-lint')()
    )
